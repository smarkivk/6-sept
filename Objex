public class Car {//basic examples 
    private String make;
    private String model;
    
    public Car(String make, String model) {
        this.make = make;
        this.model = model;
    }
    
    public void startEngine() {
        System.out.println("Starting the engine of a " + make + " " + model);
    }
}

public class Main {
    public static void main(String[] args) {
        // Creating objects of the Car class
        Car myCar = new Car("Toyota", "Camry");
        Car yourCar = new Car("Honda", "Civic");
        
        // Accessing variables and invoking methods using objects
        System.out.println("My car is a " + myCar.make + " " + myCar.model);
        myCar.startEngine();
        
        System.out.println("Your car is a " + yourCar.make + " " + yourCar.model);
        yourCar.startEngine();
    }
}
